{
  "name": "vue-spa-dev-with-webpack",
  "version": "1.0.0",
  "description": "vue app spa develop with webpack  (csr mode)",
  "author": "yemiancheng <ymc-github@gmail.com>",
  "private": true,
  "scripts": {
    "dev:csr": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "exit \"0\"",
    "test:e2e": "exit \"0\"",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs --fix",
    "lint:no-fix": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.vue\"",
    "precommit": "lint-staged",
    "build:csr-client": "rimraf dist && node build/build.js",
    "build:csr": "npm run build:csr-client",
    "start:csr-server": "node csr-server",
    "pro:csr": "node csr-server"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "es6-promise": "^4.2.8",
    "md5": "^2.2.1",
    "qs": "^6.9.1",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.1",
    "css-loader": "^0.28.11",
    "cssnano": "^4.1.10",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^4.7.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^2.30.1",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "node-notifier": "^5.4.3",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "optimize-css-assets-webpack-plugin": "^3.2.1",
    "ora": "^1.4.0",
